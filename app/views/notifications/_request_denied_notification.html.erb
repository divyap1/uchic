<li class="media">
    <%= link_to notification_path(notification, return_to: user_activity_feed_url), class: "btn btn-link pull-right", method: 'delete' do %>
    <i class="glyphicon glyphicon-remove"></i>
    <% end %>
    <div class="media-left">
      <%= image_tag "notification/denied.png" %>
    </div>
    <div class="media-body">
      <h3 class = "media-heading">Someone declined your request for a commission</h3>
        <h4><%= "#{notification.about_user.name} rejected your request for #{notification.commission.name}" %></h4>
    </div>
</li>
